    <parentFile xmlFilename="$pdlcdir/forestryPack/objects/pallets/floorTilesPallet/floorTilesPallet.xml">
        <attributes>
            <!-- update path -->
            <set path="vehicle.base.filename" value="$pdlcdir/forestryPack/objects/pallets/floorTilesPallet/floorTilesPallet.i3d"/>

            <!-- update weight -->
            <set path="vehicle.base.components#maxMass" value="100"/>
            <set path="vehicle.base.components.component(0)#mass" value="100"/>

            <!-- update capacity -->
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit(0)#capacity" value="5000"/>
            <set path="vehicle.dischargeable.dischargeNode(0)#emptySpeed" value="2000"/>
        </attributes>
    </parentFile>

    <!-- ONLY FOR ICON GENERATION -->
    <storeData>
        <name>floorTilesPallet</name>
        <image>store_floorTilesPallet.dds</image>
        <brand>LIZARD</brand>
        <category>pallets</category>
    </storeData>
</vehicle>
